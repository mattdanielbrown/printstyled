{
	"author": {
		"name": "Matt Daniel Brown",
		"email": "developer@mattdanielbrown.com",
		"url": "mattdanielbrown.com"
	},
	"bin": {
		"printstyled": "cli.js"
	},
	"dependencies": {
		"ansi-color": "^0.2.1",
		"chalk": "^4.1.0",
		"cli-color": "^2.0.0",
		"control-sequence-introducer": "^0.0.2",
		"meow": "^10.0.0",
		"np": "^8.0.0"
	},
	"description": "CLI tool to intuitively colorize and stylize text in Bash and Javascript Terminals.",
	"devDependencies": {
		"ava": "^4.0.0",
		"eslint": "^7.15.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.25.0",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"xo": "^0.41.0"
	},
	"engines": {
		"node": ">=8"
	},
	"files": [
		"index.js",
		"cli.js",
		"util/help-message-content.js",
		"util/other-text-styling.js",
		"util/parse-options.js",
		"util/colorize-text.js"
	],
	"keywords": [
		"cli-app",
		"cli",
		"colors",
		"styles",
		"print"
	],
	"license": "MIT",
	"name": "printstyled",
	"repository": "mattdanielbrown/printstyled",
	"scripts": {
		"test": "xo && ava"
	},
	"version": "1.3.4",
	"eslintConfig": {
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"unicorn/no-abusive-eslint-disable": "off"
		},
		"overrides": [
			{
				"files": [
					"bin/*.js",
					"lib/*.js"
				],
				"excludedFiles": [
					"*.test.js",
					"util/other-text-styling.js"
				],
				"rules": {
					"unicorn/no-abusive-eslint-disable": "off",
					"": [
						"error",
						"single"
					]
				}
			}
		]
	},
	"eslintIgnore": [
		"util/other-text-styling.js"
	]
}
